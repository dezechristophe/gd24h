<?xml version='1.0' encoding='utf-8'?>
<widget id="fr.deze.departs" version="2.8.2" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <feature name="Whitelist">
        <param name="android-package" value="org.apache.cordova.whitelist.WhitelistPlugin" />
        <param name="onload" value="true" />
    </feature>
    <name>Golf des 24 heures</name>
    <description>
Application du golf des 24 heures   
 </description>
    <author email="christophedeze@gmail.com" href="http://golfdes24heures.fr">
        Golf des 24 heures
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <allow-intent href="market:*" />
    <icon src="icon.png" />
    <preference name="loglevel" value="DEBUG" />
</widget>
